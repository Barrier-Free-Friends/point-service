{
  "family": "point-service-id",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "2048",
  "executionRoleArn": "arn:aws:iam::384722508472:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::384722508472:role/ecsTaskRole",
  "containerDefinitions": [
    {
      "name": "point-service-container",
      "image": "PLACEHOLDER_IMAGE",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 4000,
          "protocol": "tcp"
        }
      ],
      "environment": [
        { "name": "ENV_DB_URL", "value": "PLACEHOLDER_DB_URL" },
        { "name": "ENV_DB_USERNAME", "value": "PLACEHOLDER_DB_USERNAME" },
        { "name": "ENV_DB_PASSWORD", "value": "PLACEHOLDER_DB_PASSWORD" },
        { "name": "ENV_DB_DDL_AUTO", "value": "update" }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/point-service-id",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}